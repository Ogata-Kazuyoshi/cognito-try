{
  "family": "ogata-dummy-task-family",
  "containerDefinitions": [
    {
      "name": "ogata-dummy-container",
      "image": "temporary",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ogata-dummy-container",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "secrets": [
        {
          "name": "POSTGRES_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:157094121738:secret:ogata-cloudformation-secret-v8jgfk:username::"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:157094121738:secret:ogata-cloudformation-secret-v8jgfk:password::"
        }
      ]
    }
  ],
  "executionRoleArn": "arn:aws:iam::157094121738:role/ogata-ecs-task-execution-role",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024"
}
